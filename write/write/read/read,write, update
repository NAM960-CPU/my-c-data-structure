#include<stdio.h>
int main()
{
    int a[]={5,9,2,7,3};
    int n = sizeof(a) / sizeof(a[0]);
    int index=2;
    int newval=99;
    if(index>=0 && index <n){
        a[index]=newval;
        for(int i=0;i<n;i++) printf("%d ",a[i]);
        printf("\n");
    }
    else
    {
        printf("index out of range\n");
    }
    return 0;
    }
    
