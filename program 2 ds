#include <stdio.h>
#include <stdlib.h>

char* my_strdup(char *src) {
    int len = 0;
    while (src[len] != '\0') 
        len++;

    char *dup = (char *)malloc(len + 1);  
    if (dup == NULL)
        return NULL;  

    for (int i = 0; i <= len; i++)  
        dup[i] = src[i];

    return dup;
}

int main() {
    char str[] = "Hello!";
    char *copy = my_strdup(str);

    if (copy) {
        printf("Original: %s\n", str);
        printf("Copy: %s\n", copy);
        free(copy); 
    }

    return 0;
}

