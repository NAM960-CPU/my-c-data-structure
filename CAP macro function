//Fixed capacity by using macro function  called cap
#include<stdio.h>
#define CAP 40
void mansa(int a[], int n);
void mansa(int a[CAP],int n)
{
    printf("[");
    for(int i=0;i<n&&i<CAP;i++)
    { 
    a[i]=i;
    printf("%d", i );
    
    }
    printf("]\n");
}
int main()
{
int a[CAP],n=10;
mansa(a,n);
    
}

